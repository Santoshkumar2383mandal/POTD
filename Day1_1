class Solution {
    int print2largest(int arr[], int n) {
        
        
        Arrays.sort(arr);
        for(int i=arr.length-1;i>=1;i--)
        {
            if(arr[i]>arr[i-1])
            return arr[i-1];
            
        }
        if(arr[0]>arr[1])
        {
            return arr[0];
        // code here
        }
        else
        return -1;
    }
}
